{
  "name": "emoticontext thinks", //change this to change the alert box title text
  "version": "0.1",
  "description": "Emoticontext - sentiment analysis",
  "permissions": [],
  "background": {
    "scripts": ["background.js"],
    "persistent": false
  },
  "manifest_version": 2,
  "content_scripts": [
    {
      "matches": ["<all_urls>"],
      "js": ["content.js"]
    }
  ],
  "browser_action": {
    "default_icon": {
      "16": "icons/Smile.png"
    }
  },
  "icons": {
    "default": "images/get_started16.png",
    "32": "images/get_started32.png",
    "48": "images/get_started48.png",
    "128": "images/get_started128.png"
  }
}
